<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maximiliano.pfc" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".PantallaLauncher"
            android:label="@string/title_activity_pantalla_launcher" >
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".IniciarSesion"
            android:label="@string/title_activity_iniciar_sesion" >
        </activity>
        <activity
            android:name=".Registrarse"
            android:label="@string/title_activity_registrarse" >
        </activity>
        <activity
            android:name=".PrincipalSocios"
            android:label="@string/title_activity_principal_socios" >
        </activity>
        <activity
            android:name=".PrincipalNoSocios"
            android:label="@string/title_activity_principal_no_socios" >
        </activity>
    </application>

</manifest>
